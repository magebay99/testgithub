<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magebay_Marketplace::marketplace" title="Magebay Marketplace" sortOrder="100">
                    <resource id="Magebay_Marketplace::manage_products" title="Manager Products" sortOrder="1" />
                    <resource id="Magebay_Marketplace::manage_sellers" title="Manage Sellers" sortOrder="2" />
                    <resource id="Magebay_Marketplace::manage_commission" title="Commission and Amounts" sortOrder="3" />
                    <resource id="Magebay_Marketplace::manage_transactions" title="Sellers Transaction" sortOrder="4" />
                    <resource id="Magebay_Marketplace::manage_reviews" title="Reviews and Ratings" sortOrder="5" />
                    <resource id="Magebay_Marketplace::manage_uploadproducts" title="Upload Products" sortOrder="6" />
                    <resource id="Magebay_Marketplace::configuration" title="Configurations" sortOrder="7" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magebay_Marketplace::system_config" title="Magebay Marketplace" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>